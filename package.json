{
  "name": "@dcommerce/nextjs-swc-shop",
  "version": "0.1.0",
  "private": true,
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  },
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "husky:prepare": "husky install",
    "commit": "./node_modules/cz-customizable/standalone.js",
    "clean": "find . -name 'node_modules' -type d -prune -exec rm -rf '{}' +"
  },
  "dependencies": {
    "@apollo/client": "^3.8.7",
    "@apollo/experimental-nextjs-app-support": "^0.5.1",
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@reduxjs/toolkit": "^1.9.7",
    "http-proxy-middleware": "^2.0.6",
    "lodash-es": "^4.17.21",
    "react-redux": "^8.1.3",
    "redux-logger": "^3.0.6",
    "ts-invariant": "^0.10.3"
  },
  "devDependencies": {
    "@dcommerce/shop-commitlint-config": "workspace:^",
    "@dcommerce/shop-eslint-config": "workspace:^",
    "@dcommerce/shop-prettier-config": "workspace:^",
    "@dcommerce/shop-stylelint-config": "workspace:^",
    "@dcommerce/shop-ts-config": "workspace:^",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.10.0",
    "@types/redux-logger": "^3.0.12",
    "cz-customizable": "^7.0.0",
    "eslint": "^8.2.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "turbo": "^1.6.3",
    "typescript": "^5.2.2"
  },
  "packageManager": "pnpm@8.9.0",
  "engines": {
    "node": ">=18"
  }
}
